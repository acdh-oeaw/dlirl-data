  <text>
      <body>
         <entry xml:id="eta-ys"
            type="mainEntry"
            xml:lang="en"
            prev="#"
            next="#"
            source="Yalkut Shimoni">
            <form type="lemma">
               <orth type="lemma" xml:lang="jpa" opt="false" select="true">איטא</orth>
               <orth type="transliteration" xml:lang="fjb" opt="false">ʾyṭh</orth>
               <pron notation="rabbinic" opt="false">iṭaʾ</pron>
               <gramGrp>
                  <gram type="pos">noun</gram>
                  <gram type="gen">f.</gram>
               </gramGrp>
               <form type="variant">
                  <orth type="variant" xml:lang="jpa">איטה</orth>
                  <orth type="transliteration" xml:lang="fjb">ʾyṭh</orth>
                  <pron notation="rabbinic">iṭah</pron>
               </form>
               <form type="variant">
                  <orth type="variant" xml:lang="jpa">היטה</orth>
                  <orth type="transliteration" xml:lang="fjb">hyṭh</orth>
                  <pron notation="rabbinic">hiṭah</pron>
               </form>
            </form>
            <sense xml:id="eta-ys-1">
               <cit type="translation" xml:lang="en" subtype="sense">
                  <quote>letter „eta“</quote>
               </cit>
               <cit type="translationEquivalent" xml:lang="grc">
                  <quote>ήτα</quote>
               </cit>
               <cit type="example" xml:lang="jpa">
                  <quote>מדידכון אנא ממטי לכון זיטא-איפטא, איטא-אוקטו</quote>
                  <bibl>
                     <ref type="witness">Yalq 20</ref>
                     <ref type="witness">I 64: 82</ref>
                  </bibl>
                  <note>= <bibl type="ancient">GenR 14,2 [I 127: 3]</bibl>; <bibl type="ancient">GenR 20,6 [I 189: 3]</bibl></note>
               </cit>
               <cit type="example" xml:lang="jpa">
                  <quote>אמ' להן בלשון יוונית זיטא איפטא, איטא אוקטו</quote>
                  <bibl>
                     <ref type="witness">Yalq 692</ref>
                     <ref type="witness">VII 16: 32</ref>
                  </bibl>
                  <note>= <bibl type="ancient">GenR 14,2 [I 127: 3]</bibl>; <bibl type="ancient">GenR 20,6 [I 189: 3]</bibl>; <bibl type="ancient">NumR 4,3</bibl>; <bibl type="ancient">TanhB Bemidbar 21</bibl>; <bibl type="ancient">Tanh Bemidbar 18</bibl> (corr.) <term xml:lang="jpa">אוטה</term> <term xml:lang="fjb">ʾwṭn</term>; <bibl type="ancient">yYev 5d: 7</bibl> (corr.)</note>
               </cit>
            </sense>
            <note>Cf. GenR <ref type="entry" xml:lang="jpa" target="zeta.html">זיטא</ref>.</note>
            <ref type="bibliography">
               <bibl>
                  <title>Krauss, LW II</title>
                  <citedRange>31b</citedRange>
               </bibl>
            </ref>
         </entry>
