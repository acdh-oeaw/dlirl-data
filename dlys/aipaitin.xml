            source="Yalkut Shimoni">
            <form type="lemma">
               <orth type="lemma" xml:lang="jpa" opt="false" select="true">איפייטין</orth>
               <orth type="transliteration" xml:lang="fjb" opt="false">ʾypyyṭyn</orth>
               <pron notation="rabbinic" opt="false">aipayṭin</pron>
               <gramGrp>
                  <gram type="pos">noun</gram>
                  <gram type="num">pl.</gram>
               </gramGrp>
               <form type="variant">
                  <orth type="variant" xml:lang="jpa">איפייטון</orth>
                  <orth type="transliteration" xml:lang="fjb">ʾypyyṭwn</orth>
                  <pron notation="rabbinic">aipayṭon</pron>
               </form>
            </form>
            <sense xml:id="aipaitin-ys-1">
               <cit type="translation" subtype="sense" xml:lang="en">
                  <quote>(meaning unknown)</quote>
               </cit>
               <cit type="example" xml:lang="jpa">
                  <quote>אמרה אסתר יודעת אני שהוא דוכס ובניו 
                     <app>
                        <lem>איפייטין</lem>
                        <rdg wit="#W">איטרטיגין</rdg>
                        <rdg wit="#L">איסייתין</rdg>
                        <rdg wit="#V">איסייטין</rdg>
                     </app> בכל העולם</quote>
                  <ref type="entry" xml:lang="jpa">דוכס</ref>
                  <ref type="entry" xml:lang="jpa">איטרטיגין</ref>
                  <bibl>
                     <ref type="witness">Yalq Es 1058</ref>
                     <ref type="witness">531c: 13</ref>
                  </bibl>
                  <note>= <bibl type="ancient">SAME(B) 6 [36b]</bibl> (<term xml:lang="jpa">איפייטון</term>)</note>
               </cit>
            </sense>
            <note>Unclear. Buber in <bibl type="ancient">SAME</bibl> suggests corrupted <term xml:lang="jpa">אסטרטליט</term></note>
         </entry>
