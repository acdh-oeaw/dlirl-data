         <entry xml:id="mastikhe-dlgenr"
            type="mainEntry"
            xml:lang="en"
            prev="#mantilin-ys"
            next="#mastukha-ys"
            source="Genesis Rabbah">
            <form type="lemma">
               <orth type="lemma" xml:lang="jpa" opt="false" select="true">מסטיכי</orth>
               <orth type="transliteration" xml:lang="fjb" opt="false">msṭyky</orth>
               <pron notation="rabbinic">masṭikhe</pron>
               <gramGrp>
                  <gram type="pos">noun</gram>
                  <gram type="gen">f.</gram>
               </gramGrp>
               <form type="variant">
                  <orth type="variant" xml:lang="jpa">מסתיכי</orth>
                  <orth type="transliteration" xml:lang="fjb">mstyky</orth>
                  <pron notation="rabbinic">mastikhe</pron>
               </form>
            </form>
            <sense xml:id="mastikhe-dlgenr-1">
               <usg type="domain" opt="false">medicine/food/arts</usg>
               <usg type="socioCultural" norm="borrowing" opt="false">cultural borrowing</usg>
               <usg type="normativity" opt="false">standard</usg>
               <usg type="meaningType" opt="false">lit.</usg>
               <usg type="geographic" opt="false">Syriac Aramaic, Coptic</usg> 
               <cit type="translation" xml:lang="en" subtype="sense">
                  <quote>mastic gum</quote>
               </cit>
               <cit type="translationEquivalent" xml:lang="grc">
                  <quote>μαστίχη</quote>
               </cit>
               <cit type="example" xml:lang="jpa">
                  <quote>ולוט 
                     <app>
                        <lem>מסטיכי</lem>
                        <rdg wit="#Vat1">מסתיכי</rdg>
                        <rdg wit="#K V">מוצטכא</rdg>
                        <rdg wit="#Ox1">שטרי</rdg>
                        <rdg wit="#Ox2">ששרי</rdg>
                     </app> עבידה בול</quote>
                  <bibl>
                     <ref type="witness">91,11</ref>
                     <ref type="witness">1136: 1</ref>
                  </bibl>
                  <cit type="translation" xml:lang="en">
                     <quote>And “loṭ” (Gen 43,11): mastic gum formed into a lump.</quote>
                  </cit>
                  <note>= <bibl type="ancient">Yalq 149 [II 49: notes]</bibl> (<term xml:lang="jpa">מצטוכא</term>); <bibl type="ancient">SekhT 37,25 [220]</bibl> (<term xml:lang="jpa">מסטיני</term>)</note>
               </cit>
            </sense>
               <etym type="borrowing" opt="false">
                  <lbl opt="false">source language</lbl>
                  <lang opt="false">Classical Greek</lang>
                  <cit type="etymon" xml:lang="grc">
                     <xr type="related" opt="false">
                        <lbl opt="false">Cf. LSJ</lbl>
                        <ref target="https://www.perseus.tufts.edu/hopper/text?doc=Perseus%3Atext%3A1999.04.0057%3Aentry%3Dmasti%2Fxh"
                           type="entry">μαστίχη</ref>
                     </xr>
                  </cit>
               </etym>
            <note>Cf. <bibl type="modern">Krauss, LW II 346a</bibl>; <bibl type="modern">Löw (1881): 70</bibl>; <bibl type="modern">Löw, Flora I 127</bibl>. Cf. Syr. <term xml:lang="syc">ܡܣܛܝܟܝ</term> <term xml:lang="fjb">msṭyky</term>. cf. <ref type="entry" xml:lang="jpa" target="mastukha-ys.html">מצטוכא</ref> Yalq.</note>
            <ref type="bibliography">
               <bibl>
                  <title>Sokoloff, DJPA</title>
                  <citedRange>352b</citedRange>
               </bibl>
            </ref>
